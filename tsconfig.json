{
    "compilerOptions": {
      "outDir": "./dist/lib",
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "lib": ["es5", "es6", "dom"],
      "allowJs": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "types": [
      
      ]
    },
    "include": [
      "src/lib"
    ],
    "exclude": [
      "src/docs/**/*",
      "src/server/**/*",
      "src/server",
      "dist/server",
      "dist/**/*",
      "node_modules"
    ]
  }